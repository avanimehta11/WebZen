{
  "manifest_version": 3,
  "name": "WebZen",
  "version": "0.0.1",
  "description": "Social Media Blocker",
  "author": "Avani Mehta",
  "icons": {
    "1024": "icon.png"
  },
  "action": {
    "default_icon": "icon.png"
